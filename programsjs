<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=<<device-width>>, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
       /* let num1=parseInt(prompt("Enter a num1"));
       if((num1%2==0) && (num1%3==0) ){
            console.log(num1+"is divisible by 2 & 3");
            document.write("divisible by 2 & 3");
        }else{
            console.log("none");
        }*/


       /* let num2=parseInt(prompt("Enter a num2"));
        let num3=parseInt(prompt("Enter a num3"));
        let num4=parseInt(prompt("Enter a num4"));
        if((num2>num3) && (num2>num4)  ){
            console.log("num2 is divsion");
        }else if((num3>num2) && (num3>num4)){
            console.log("num3 is large");

        }else{
            console.log("num4 is large")
        }*/

       /* let num5=parseInt(prompt("Enter number of working days"));
        let num6=parseInt(prompt("Enter a  noOf adsent"));
        let present=num5-num6;

        let per = (present/num5)*100;
        if(per>=75){
            console.log("eligible for exam");
        }else{
            console.log("none")
        }*/

       /* let salary=parseInt(prompt("Enter a salary"));
        let tp=parseInt(prompt("Enter a time period"));
        if(tp>10){
            console.log(((salary*10)/100)+salary);
        }else if(tp>=6 && tp<=10){
            console.log(((salary*8)/100)+salary)
        }else if(tp<6){
            console.log(((salary*5)/100)+salary)
            
        }*/

        for(let i=10;i<=15;i++){
            console.log(i)
            document.write(i+ "</br>")

        }

        for(let j=2;j<=9;j=j+2){
            console.log(j+3)  
            <!--j=2:2+3=5;  j=5+2=7; j=7+2=9; j=9+2=11; j=11+2=15 -->
        }

        for(let k=1 ; k<=5;k++){
            
            document.write("chiru"+"</br>")
        }
        let m=9;

        for(let l=1;l<=10;l++){
            document.write("9*"+l+"="+m*l+ "</br>")
        }

        const a="venkata";
        for(let b in a){
            console.log(a[b]);

        }

        let c=2 
        let d=8;
        while(c<=d){
            console.log(c+1)
            c=c+2
        }

       /* c=2 2<=8 2+1=3 c=3+2=5*/

       let e=0;
       do{
        console.log(e)
        e++
    }while(c<5);

    for(let i=2;i<=8; i=i+2){
        if(i>4){
            break;
        }console.log(i+1)
    }

    /*i=2 2<=8 2+1=3 i=4
      i=4 4<=8 4+1=5 i=6*/

      for(let i=2;i<=8;i=i+1){
        if(i>5){
            continue
        }
        console.log(i+2)
        document.write(i+2)
      }
      /*i=2 2<=8 2+2=4 i=2+1=3
      i=3 3<=8 3+2=5 i=3+1=4
      i=4  4<=8 
      */

      for(let i=1;i<=3;i++){
        document.write(i+"</br>");
        for(let j=1;j<=4;j++){
            document.write(j," ");
        }
        document.write("<br>")

      }
    for(let i=1;i<=5;i++){
        for(let j=1;j<=i;j++){
            document.write("*")
           
        }
        document.write("<br>");
    }

    for(let i=1;i<=5;i--){
        for(let j=i;j<=i;j--){
            document.write("*")
        }
        document.write("<br>")
    }




        


    </script>

</body>
</html>
