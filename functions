<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <script>
       /* function add(){
            console.log(a+b);
        }
        let a=parseInt(prompt("Enter a num1"));
        let b=parseInt(prompt("Enter a num2"))
        add();


        function greet(){
            console.log("hi ramu")
        }

        greet()

        function add1(num1,num2){
            return num1+num2;
        }
        let c=parseInt(prompt("Enter a num1"));
        let d=parseInt(prompt("Enter a num2"))
        let result=add1(c,d);
        console.log(result)*/


       /* let num1=parseInt(prompt("Enter a num1"));
        if(num1%2==0 ){
            console.log("its is even");
        }else{
            console.log("not a even")
        }*/

        let f=()=>{
            console.log("hi rama")
        }
        f()

        let x=(x,y)=>{
            console.log(x+y)
         }
         x(30,20)
         x(40,50)

         let add=(a,b)=>{
            console.log(a+b)
         }
         let num1=parseInt(prompt("Enter a num1"));
         let num2=parseInt(prompt("Enter a num2"))
         add(num1,num2)

         let greet=x=>console.log(x);
         greet("hello")

         let age =parseInt(prompt("Enter a age"))
         let agecheck=(age<18)?
         ()=>console.log("vote"):
         ()=>console.log("no vote")
         agecheck()

         console.log("water boiled");
         setTimeout(()=>{
            console.log("start cooking")
         },5000);
         console.log("cut the veg")

         function greet1(name,callback){
            console.log(`Hello,${name}!`)
            callback();
         }
     function saygoodbye(){
        console.log("GoodBye")
     }
     greet1("rama,sayGoodbye")
    </script>
