        document.write(name) 
        let a=1
        console.log(a)

        var  b=2;
        document.write(typeof (b));
        document. write(b);
        console.log(b)


        let c=3.4;
        document.write(typeof (c));
        document.write(c);
        console.log(c);

        var d="venkata";
        document.write(d)
        console.log(d);
        document.write(typeof(d));
        console.log(typeof(d))

    console.log(c+`hi`+d);
    console.log(d+name)
    console.log(name+c);
    console.log(b+/"hi"/+name)

