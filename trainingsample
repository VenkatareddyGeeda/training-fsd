<body>
    <script>
         var name="Venkata"
        var  name="raja"
        console.log(name)
        document.write(name) 
        let a=1
        console.log(a)

        var  b=2;
        document.write(typeof (b));
        document. write(b);
        console.log(b)


        let c=3.4;
        document.write(typeof (c));
        document.write(c);
        console.log(c);

        var d="venkata";
        document.write(d)
        console.log(d);
        document.write(typeof(d));
        console.log(typeof(d))

    console.log(c+`hi`+d);
    console.log(d+name)
    console.log(name+c);
    console.log(b+/"hi"/+name)

    console.log(a+10);

   b+=10
   console.log(b);

   b-=10;
   console.log(b);

   b/=4
   console.log(b)

   b%=2;
   console.log(b)

   console.log(a>b);
   console.log(a==b);
   console.log((a<b) && (a>b));
    console.log((a<b) || (a>b));

    let f=prompt()
    let g=prompt()
    console.log(f+g);
    console.log(a)
    console.log(b)

    if(a>b){
        console.log("ture");
        document.write("ture")
    }else{
        console.log('flase')
    }
    
    let age=prompt("Enter your age");
    let m=name=prompt("enter your name");
    if(age<18){
        alert(name+"you are to young to marry");

    }else if(age>35){
        alert(name+"you are ti old to marry");


    }else{
        alert(name+"perfect age to marry");
    }

    let sa=prompt("Enter score A");
    let sb=prompt('Enter score B');
    let sum=sa+sb;
    if((sa>300 || sb>300) && (sum<500) ){
        console.log("team up");
    }else{
        console.log('cannot teamup');
    }




    </script>
    
